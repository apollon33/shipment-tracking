    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="ar" value="[$clip$$END$]" description="script tag" toReformat="true" toShortenFQNames="true">
    <variable name="clip" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
