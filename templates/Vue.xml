  <template name="keb" value="$kebab$" description="to kebab" toReformat="true" toShortenFQNames="true">
    <variable name="kebab" expression="lowercaseAndDash(clipboard())" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="VUE_TEMPLATE" value="true" />
    </context>
  </template>
  <template name="cam" value="$camel$" description="to snake" toReformat="true" toShortenFQNames="true">
